<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.electromob.faculty_management_system">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".student_home"></activity>
        <activity android:name=".faculty_home" />
        <activity android:name=".forgot_password" />
        <activity android:name=".faculty_signup_activity" />
        <activity android:name=".student_signup_activity" />
        <activity android:name=".login_activity" />
        <activity android:name=".faculty_login_activity" />
        <activity android:name=".student_login_activity" />
        <activity android:name=".signup_activity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>